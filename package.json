{
  "name": "typescript-note",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "nodemon -w '../../packages/slidev/dist/*.mjs' --exec \"slidev ./slides.md --open=false --log=info\"",
    "export": "slidev export --with-clicks",
    "export:pdf": "npm run export -- --with-toc --format pdf --output typescript-note",
    "export:pptx": "npm run export -- --format pptx --output typescript-note.pptx"
  },
  "dependencies": {
    "@slidev/cli": "^0.49.10",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@babel/standalone": "^7.24.6",
    "@iconify-json/logos": "^1.1.42",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/ri": "^1.1.21",
    "@slidev/types": "^0.49.3",
    "@vue/compiler-sfc": "^3.4.27",
    "nodemon": "^3.1.0",
    "playwright-chromium": "^1.44.1",
    "prettier": "^3.3.1",
    "prettier-plugin-slidev": "^1.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
